{
  "name": "gfed-medusa-storefront",
  "private": true,
  "version": "0.0.1",
  "scripts": {
    "build": "turbo run build",
    "check-types": "turbo run check-types",
    "dev": "concurrently \"tsx local-proxy.ts\" \"turbo run dev --concurrency=15\"",
    "dev:proxy": "tsx local-proxy.ts",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "lint": "turbo run lint",
    "lint:workflows": "yamllint .github/workflows/*.yaml && actionlint .github/workflows/*.yaml",
    "prepare": "husky",
    "test": "turbo run test",
    "ci:publish": "pnpm build --filter=\"./packages/**\" && changeset publish --access public",
    "changeset": "changeset",
    "changeset:status": "changeset status",
    "worker:dev": "pnpm --filter @gfed-medusa/sf-gateway dev",
    "worker:deploy:smoke": "pnpm --filter @gfed-medusa/sf-gateway deploy:smoke",
    "worker:deploy:qa": "pnpm --filter @gfed-medusa/sf-gateway deploy:qa",
    "worker:deploy:production": "pnpm --filter @gfed-medusa/sf-gateway deploy:production",
    "worker:tail:smoke": "pnpm --filter @gfed-medusa/sf-gateway tail:smoke",
    "worker:tail:qa": "pnpm --filter @gfed-medusa/sf-gateway tail:qa",
    "worker:tail:production": "pnpm --filter @gfed-medusa/sf-gateway tail:production"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.8",
    "@types/express": "^5.0.6",
    "concurrently": "^8.2.2",
    "express": "^5.2.1",
    "htmlrewriter": "^0.0.13",
    "husky": "^9.1.7",
    "prettier": "^3.6.2",
    "tsx": "^4.7.1",
    "turbo": "^2.6.2",
    "typescript": "5.9.2"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=18"
  }
}
