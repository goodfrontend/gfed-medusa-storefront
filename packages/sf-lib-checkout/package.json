{
  "name": "@gfed-medusa/sf-lib-checkout",
  "version": "3.0.0",
  "private": false,
  "type": "module",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "license": "MIT",
  "files": [
    "dist",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/goodfrontend/gfed-medusa-storefront.git",
    "directory": "packages/sf-lib-checkout"
  },
  "exports": {
    "./components/*": {
      "types": "./dist/components/*/index.d.ts",
      "default": "./dist/components/*/index.js"
    },
    "./lib/*": {
      "types": "./dist/lib/*.d.ts",
      "default": "./dist/lib/*.js"
    },
    "./templates/*": {
      "types": "./dist/templates/*/index.d.ts",
      "default": "./dist/templates/*/index.js"
    }
  },
  "scripts": {
    "build": "tsdown",
    "codegen": "graphql-codegen --config codegen.ts",
    "codegen:watch": "graphql-codegen --config codegen.ts --watch",
    "dev": "tsc --watch",
    "format:fix": "prettier --write .",
    "test": "jest --passWithNoTests",
    "check-types": "tsc --noEmit",
    "lint": "eslint . --max-warnings 55"
  },
  "dependencies": {
    "@apollo/client": "^4.0.4",
    "@gfed-medusa/sf-lib-common": "^3.0.0",
    "@gfed-medusa/sf-lib-ui": "^1.2.1",
    "@headlessui/react": "^2.2.9",
    "@medusajs/icons": "^2.12.1",
    "@medusajs/ui": "^4.0.29",
    "@medusajs/types": "^2.12.1",
    "@stripe/react-stripe-js": "^1.7.2",
    "@stripe/stripe-js": "^1.29.0",
    "lodash": "^4.17.21",
    "server-only": "^0.0.1"
  },
  "peerDependencies": {
    "next": "^15.3.6",
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^6.0.0",
    "@graphql-codegen/typed-document-node": "^6.0.0",
    "@graphql-codegen/typescript": "^5.0.0",
    "@graphql-codegen/typescript-operations": "^5.0.0",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@packages/eslint-config": "workspace:*",
    "@packages/jest-config": "workspace:*",
    "@packages/prettier-config": "workspace:*",
    "@packages/typescript-config": "workspace:*",
    "@parcel/watcher": "^2.5.1",
    "@tailwindcss/postcss": "^4",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "6.8.0",
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^29.5.2",
    "@types/lodash": "^4.14.195",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "dotenv": "^17.2.3",
    "eslint": "^9",
    "eslint-config-next": "16.0.6",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "tailwindcss": "^4",
    "tsdown": "^0.17.2",
    "typescript": "^5"
  }
}
