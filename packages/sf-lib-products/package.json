{
  "name": "@gfed-medusa/sf-lib-products",
  "version": "1.2.0",
  "private": false,
  "type": "module",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "license": "MIT",
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/goodfrontend/gfed-medusa-storefront.git",
    "directory": "packages/sf-lib-products"
  },
  "exports": {
    "./components/*": {
      "types": [
        "./dist/components/*/index.d.ts",
        "./dist/components/*/*.d.ts"
      ],
      "default": [
        "./dist/components/*/index.js",
        "./dist/components/*/*.js"
      ]
    },
    "./templates/*": {
      "types": [
        "./dist/templates/*/index.d.ts",
        "./dist/templates/*/*.d.ts"
      ],
      "default": [
        "./dist/templates/*/index.js",
        "./dist/templates/*/*.js"
      ]
    },
    "./lib/*": {
      "types": "./dist/lib/*.d.ts",
      "default": "./dist/lib/*.js"
    },
    "./types/*": {
      "types": "./dist/types/*.d.ts",
      "default": "./dist/types/*.js"
    }
  },
  "scripts": {
    "build": "tsdown",
    "codegen": "graphql-codegen --config codegen.ts",
    "codegen:watch": "graphql-codegen --config codegen.ts --watch",
    "dev": "tsc --watch",
    "format:fix": "prettier --write .",
    "check-types": "tsc --noEmit",
    "lint": "eslint . --max-warnings 40"
  },
  "dependencies": {
    "@apollo/client": "^4.0.9",
    "@gfed-medusa/sf-lib-common": "^2.2.0",
    "@gfed-medusa/sf-lib-ui": "^1.2.0",
    "@headlessui/react": "^2.2.9",
    "@medusajs/icons": "^2.12.1",
    "@medusajs/types": "^2.12.1",
    "@medusajs/ui": "^4.0.29",
    "@radix-ui/react-accordion": "^1.2.1",
    "isomorphic-dompurify": "^2.34.0",
    "lodash": "^4.17.21",
    "next": "^15.3.8",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "server-only": "^0.0.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^6.0.0",
    "@graphql-codegen/typed-document-node": "^6.0.0",
    "@graphql-codegen/typescript": "^5.0.0",
    "@graphql-codegen/typescript-operations": "^5.0.0",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@packages/eslint-config": "workspace:*",
    "@packages/jest-config": "workspace:*",
    "@packages/prettier-config": "workspace:*",
    "@packages/typescript-config": "workspace:*",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "16.0.6",
    "tsdown": "^0.17.2",
    "typescript": "^5"
  }
}
