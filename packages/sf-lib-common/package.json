{
  "name": "@gfed-medusa/sf-lib-common",
  "version": "2.4.0",
  "private": false,
  "type": "module",
  "license": "MIT",
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/goodfrontend/gfed-medusa-storefront.git",
    "directory": "packages/sf-lib-common"
  },
  "exports": {
    "./components/*": {
      "types": [
        "./dist/components/*/index.d.ts",
        "./dist/components/*/*.d.ts"
      ],
      "default": [
        "./dist/components/*/index.js",
        "./dist/components/*/*.js"
      ]
    },
    "./lib/*": {
      "types": "./dist/lib/*.d.ts",
      "default": "./dist/lib/*.js"
    },
    "./types/*": {
      "types": "./dist/types/*.d.ts",
      "default": "./dist/types/*.js"
    }
  },
  "scripts": {
    "build": "tsdown",
    "codegen": "graphql-codegen --config codegen.ts",
    "codegen:watch": "graphql-codegen --config codegen.ts --watch",
    "dev": "tsdown --watch",
    "format:fix": "prettier --write .",
    "check-types": "tsc --noEmit",
    "lint": "eslint . --max-warnings 40"
  },
  "dependencies": {
    "@apollo/client": "^4.0.9",
    "@gfed-medusa/sf-lib-ui": "^1.2.1",
    "@headlessui/react": "^2.2.9",
    "@medusajs/icons": "^2.12.1",
    "@medusajs/js-sdk": "^2.12.1",
    "@medusajs/types": "^2.12.1",
    "@medusajs/ui": "^4.0.29",
    "class-variance-authority": "^0.7.1",
    "isomorphic-dompurify": "^2.34.0",
    "lucide-react": "^0.556.0",
    "react-country-flag": "^3.1.0",
    "server-only": "^0.0.1",
    "swr": "^2.3.8"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^6.0.0",
    "@graphql-codegen/typed-document-node": "^6.0.0",
    "@graphql-codegen/typescript": "^5.0.0",
    "@graphql-codegen/typescript-operations": "^5.0.0",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@packages/eslint-config": "workspace:*",
    "@packages/jest-config": "workspace:*",
    "@packages/prettier-config": "workspace:*",
    "@packages/typescript-config": "workspace:*",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "6.8.0",
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^29.5.2",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "dotenv": "^17.2.3",
    "eslint": "^9",
    "eslint-config-next": "16.0.6",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "madge": "^8.0.0",
    "tsdown": "^0.17.3",
    "typescript": "^5"
  },
  "peerDependencies": {
    "@medusajs/ui": "^4.0.29",
    "next": "^15.3.8",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwindcss": "^4.1.18"
  }
}
